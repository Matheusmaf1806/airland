{
  "version": 2,

  "buildCommand": "npm run build",

  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/worker.js",
      "use": "@vercel/node"
    }
  ],

  // 3) As rotas que vocÃª definiu
  "routes": [
    { "src": "/api/worker", "dest": "api/worker.js" },
    { "src": "/(.*)", "dest": "server.js" }
  ]
}
