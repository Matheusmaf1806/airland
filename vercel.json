{
  "version": 2,
  "buildCommand": "npm run build",
  "functions": {
    "server.js": {
      "includeFiles": "server.js",
      "runtime": "nodejs16.x"
    },
    "api/worker.js": {
      "includeFiles": "api/worker.js",
      "runtime": "nodejs16.x"
    }
  },
  "routes": [
    {
      "src": "/api/worker",
      "dest": "api/worker.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
